rule all:
    input:
        expand("{outpath}/03_variants/04_deepvariant/{sample}/{sample}.{chr}.{ref_version}.output.vcf.gz", outpath=OUTPATH, sample=SAMPLE, ref_version=REF_version, chr=CHROMOSOMES),
        expand("{outpath}/03_variants/04_deepvariant/{sample}/{sample}.{chr}.{ref_version}.output.gvcf.gz", outpath=OUTPATH, sample=SAMPLE, ref_version=REF_version, chr=CHROMOSOMES)